<%= form_for course do |f| %>
  <%=f.label :name %><br>
  <%=f.text_field :name %>
  <br>
  <%=f.label :address %><br>
  <%=f.text_field :address %>
  <br>
  <%=f.label :city %><br>
  <%=f.text_field :city %>
  <br>
  <%=f.label :state %><br>
  <%=f.text_field :state %>
  <br>
  <%=f.label :description %><br>
  <%=f.text_field :description %>
  <br>
  <%=f.label :price %><br>
  <%=f.text_field :price %>
  <br>
  <%=f.label :par %><br>
  <%=f.text_field :par %>
  <br>
  <%=f.label :length %><br>
  <%=f.text_field :length %>
  <br>
  <%=f.label :difficulty %><br>
  <%=f.text_field :difficulty %>
  <br>
  <%= f.submit submit_text %>

<% end %>



